<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hibernate.Danie" table="sys.Danie">
        <id name="id" type="java.lang.Integer" column="ID">
            <generator class="sequence">
            	<param name="sequence">danie_seq</param>
            </generator>
        </id>
        <property name="cena" type="java.lang.Float"  column="cena" />
        <property name="nazwa" type="java.lang.String" column="nazwa" />
    </class>
</hibernate-mapping>