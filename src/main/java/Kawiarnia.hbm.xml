<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hibernate.Kawiarnia" table="sys.KAWIARNIA">
        <id name="id" type="java.lang.Integer" column="ID">
            <generator class="sequence">
            	<param name="sequence">kawiarnia_seq</param>
            </generator>
        </id>
        <property name="nazwa" type="java.lang.String"  column="NAZWA" />
        <property name="adres" type="java.lang.String"  column="ADRES" />
        <property name="ilosc_gwiazdek_michelin" type="java.lang.Integer"  column="ILOSC_GWIAZDEK_MICHELIN" />
    </class>
</hibernate-mapping>