<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hibernate.Oferuje" table="sys.Oferuje">
        <composite-id>
        <key-many-to-one name="kawiarnia_id" class="hibernate.Kawiarnia"
        	column="kawiarnia_id"/>
       	<key-many-to-one name="danie_id" class="hibernate.Danie"
        	column="danie_id"/>
        </composite-id>
    </class>
</hibernate-mapping>